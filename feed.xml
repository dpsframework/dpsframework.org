---
layout: null
lang: es
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.data.dpsFramework.title[page.lang] | xml_escape }}</title>
    <subtitle>{{ site.data.dpsFramework.subtitle[page.lang] | xml_escape }}</subtitle>
    <description>{{ site.data.dpsFramework.description[page.lang] | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    
    
    
  
 
    {% assign papers    = site.papers    | sort: "year" | reverse | where:"lang", page.lang  %}
    {% assign proposals = site.proposals | sort: "date" | reverse | where:"lang", page.lang  %}
    {% assign guias     = site.guides    | where:"lang", page.lang | where:"chapter", 0 %}
    {% assign notes     = site.notes     | where:"lang", page.lang %}
    {% assign paginas   = site.pages     | where:"lang", page.lang %}
  

    {% for item in papers limit:10 %}
      <item>
        <title>{{ item.title | xml_escape }}</title>
        <description>{{ item.headtitle | xml_escape }}</description>
        <citation>{{ item.citation | xml_escape }}</citation>
        <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
        <link>{{ item.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ item.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>{{ site.data.i18n[ page.lang ].papersH1 }}</category>
      </item>
    {% endfor %}
     
    
    {% for item in proposals limit:10 %}
      <item>
        <title>{{ item.title | xml_escape }}</title>
        <description>{{ item.headtitle | xml_escape }}</description>
        <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
        <link>{{ item.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ item.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>{{ site.data.i18n[ page.lang ].proposalsH1 }}</category>
      </item>
    {% endfor %}

    
    {% for item in guias limit:10 %}
      <item>
        <title>{{ item.title | xml_escape }}</title>
        <description>{{ item.headtitle | xml_escape }}</description>
        <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
        <link>{{ item.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ item.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>{{ site.data.i18n[ page.lang ].guidesH1 }}</category>
      </item>
    {% endfor %}

    
    {% for item in notes limit:10 %}
      <item>
        <title>{{ item.title | xml_escape }}</title>
        <description>{{ item.headtitle | xml_escape }}</description>
        <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
        <link>{{ item.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ item.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>{{ site.data.i18n[ page.lang ].notesH1 }}</category>
      </item>
    {% endfor %}
    
    
    {% for item in paginas limit:25 %}
      <item>
        <title>{{ item.title | xml_escape }}</title>
        <description>{{ item.headtitle | xml_escape }}</description>
        <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
        <link>{{ item.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ item.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>dpsFramework Website pages</category>
      </item>
    {% endfor %}
    
   
    
    













  </channel>
</rss>
